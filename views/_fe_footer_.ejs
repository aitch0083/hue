				});

				$('#ArticleContentContainer').find('img').responsImg();

				$.get('/menu').then(function(result){
					$('#top-menu-placeholder').html(result);
				});

      		});

      		$(window).resize(function(){
			    $('#ArticleContentContainer').find('img').responsImg();
			    $('.cate-card').equalHeights();
